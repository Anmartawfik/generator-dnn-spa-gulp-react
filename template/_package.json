{
  "name": "<%= props.projectName %>",
  "version": "01.00.00",
  "description": "Demo module used at DNN <%= props.organization %> 2015",
  "dnnModule": {
    "fullName": "<%= props.organization %>.<%= props.projectName %>",
    "friendlyName": "<%= props.projectName %> Module",
    "packageName": "<%= props.organization %>_<%= props.projectName %>",
    "owner": {
      "name": "<%= props.name %>",
      "organization": "<%= props.organization %>",
      "url": "<%= props.url %>.org",
      "email": "<%= props.email %>"
    },
    "module": {
      "packageName": "<%= props.organization %>_<%= props.projectName %>",
      "folderName": "<%= props.organization %>/<%= props.projectName %>",
      "azureCompatible": "true",
      "iconFile": "DesktopModules\\<%= props.organization %>\\<%= props.projectName %>\\<%= props.projectName.toLowerCase() %>.png"
    },
    "tempPath": "./package",
    "packagesPath": "./_Packages",
    "pathToAssemblies": "./bin",
    "pathToScripts": "./_Installation/SQL",
    "pathToSupplementaryFiles": "./_Installation",
    "excludeFilter": [
     "node_modules",
     "bin",
     "obj"
    ]
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<%= props.organization %>/<%= props.projectName %>.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DNN-<%= props.organization %>/<%= props.projectName %>/issues"
  },
  "homepage": "https://github.com/DNN-<%= props.organization %>/<%= props.projectName %>#readme",
  "devDependencies": {
    "gulp": "^3.9.0",
    "gulp-browserify": "^0.5.1",
    "gulp-dotnet-assembly-info": "^0.1.11",
    "gulp-filter": "^3.0.1",
    "gulp-markdown": "^1.1.0",
    "gulp-minify-css": "^1.2.1",
    "gulp-msbuild": "^0.2.13",
    "gulp-plumber": "^1.0.1",
    "gulp-react": "^3.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.4.1",
    "gulp-util": "^3.0.6",
    "gulp-zip": "^3.0.2",
    "merge2": "^0.3.6",
    "react": "^0.13.3",
    "react-tools": "^0.13.3",
    "reactify": "^1.1.1"
  },
  "dependencies": {
    "jquery": "^2.1.4"
  }
}
